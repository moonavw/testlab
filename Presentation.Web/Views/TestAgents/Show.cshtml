@model TestAgent

@{
    ViewBag.Title = "Test Agent: " + Model.Name;
}

<div>
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.IsOnline)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.IsOnline)
        </dd>
        
        <dt>
            @Html.DisplayNameFor(model => model.LastTalked)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.LastTalked)
            &nbsp;
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Summary)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Summary)
        </dd>
    </dl>
</div>

<div>
    <h2 class="sub-header">Test Jobs</h2>
    @Html.Partial("_TestJobs", Model.Jobs)
</div>
