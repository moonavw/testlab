@model TestSession

@{
    ViewBag.Title = "Edit Test Session";
}


@using (Html.BeginForm("Update", "TestSessions", new { id = Model.Id, testprojectId = Model.Project.Id }))
{
    @Html.HttpMethodOverride(HttpVerbs.Put)

    @Html.Partial("_form", Model)
}

@using (Html.BeginForm("Destroy", "TestSessions", new { id = Model.Id }))
{
    @Html.HttpMethodOverride(HttpVerbs.Delete)
    
    <input type="submit" value="Delete" class="btn btn-danger" />
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
